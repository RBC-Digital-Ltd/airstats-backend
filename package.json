{
  "name": "airstats-backend",
  "version": "0.0.1",
  "description": "Backend for the Airstats Service",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "workspaces/*"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --check .",
    "typecheck": "tsc --pretty --noEmit",
    "dev": "serverless offline --reloadHandler",
    "deploy": "serverless deploy",
    "build:fetcher": "esbuild src/dataFetchers/getVatsimData/index.mts --bundle --platform=node --outfile=dist/getVatsimData.mjs --format=esm"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/RBC-Digital-Ltd/airstats-backend.git"
  },
  "author": "Rahul Parkar",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/RBC-Digital-Ltd/airstats-backend/issues"
  },
  "homepage": "https://github.com/RBC-Digital-Ltd/airstats-backend#readme",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "esbuild": "^0.18.11",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jest-extended": "^2.0.0",
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "nock": "^13.3.1",
    "prettier": "^2.8.8",
    "serverless": "^3.33.0",
    "serverless-esbuild": "^1.45.1",
    "serverless-offline": "^12.0.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "got": "^11.0.0"
  }
}
